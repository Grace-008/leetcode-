class Solution:
    def countAndSay(self, n: int) -> str:
        curr = "1"
        for i in range(1, n):
            prev = curr
            curr = ""
            say = prev[0]
            count = 1
            for j in range(1, len(prev)):
                if prev[j] == say:
                    count += 1
                else:
                    curr += str(count) + say
                    say = prev[j]
                    count = 1
            curr += str(count) + say
        return curr
