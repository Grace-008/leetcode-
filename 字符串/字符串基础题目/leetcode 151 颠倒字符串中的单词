class Solution:
    def reverseWords(self, s: str) -> str:
        #清洗数据,把多余的空格都删掉
        sb = []
        for c in s:
            #单词中的字母/数字
            if c!=' ':
                sb.append(c)
            #if the sb list is not empty or checks whether the last character appended to the sb list is not a space.  
            #单词之间保留一个空格
            elif sb and sb[-1] != ' ':
                sb.append(' ')
        #末尾如果有空格，清除之, remove any trailing space characters at the end of the cleaned string.
        if sb and sb[-1] ==' ':
            sb.pop()
        #清洗之后的字符串
        chars = sb

        #进行单词的翻转
        def reverse(l,r):
            chars[l],char[r] = chars[r],chars[l]
            l+=1
            r-=1

        n = len(chars)
        #先整体翻转
        reverse(0,n-1)

        #翻转内部单词
        start=end=0
        while end<n:
            if chars[end]==' ':
                reverse(start,end-1)
                start=end+1
            end+=1

        # 翻转最后一个单词
        reverse(start, n-1)

        # 最后得到题目想要的结果
        return "".join(chars)

