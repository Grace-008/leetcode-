class Solution:
    def mySqrt(self, x: int) -> int:
        if x<2:
            return x
        left,right=0,x//2
        ans=-1
        while left<=right:
            mid=left+(right-left)//2
            if mid*mid>x:
                right=mid-1
            else:
                ans=mid
                left=mid+1
        return ans
