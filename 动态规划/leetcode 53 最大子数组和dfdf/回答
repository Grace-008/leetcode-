⭕️暴力解法

class Solution:
    def maxSubArray(self, nums: List[int]) -> int:
        maxSum=float('-inf')
        for i in range(len(nums)):
            for j in range(i, len(nums)):
                sum = 0
                for k in range(i,j+1):
                    sum+=nums[k]
                maxSum = max(maxSum, sum)
        return maxSum
