class Solution:
    def wordBreak(self, s: str, wordDict: List[str]) -> bool:

        dp = [False] * (len(s) + 1)
        dp[0] = True

    
        # 注意顺序
        for i in range(1, len(s) + 1):
            for word in wordDict:
                wordLen = len(word)
                if i>=wordLen and s[i-wordLen:i]==word:
                    dp[i]=dp[i] or dp[i-wordLen]

        return dp[-1]
