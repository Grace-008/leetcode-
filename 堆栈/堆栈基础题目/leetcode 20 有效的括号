class Solution:
    def isValid(self, s: str) -> bool:
        dic = {')':'(',']':'[','}':'{'}
        stack = []
        for i in s:
            #如果出现的是右括号and该括号出现在dic中
            if stack and i in dic:
                if stack[-1] == dic[i]:
                    stack.pop()
                else:
                    return False
            else:
                #如果出现的是左括号
                stack.append(i)
        #stack是否empty   
        return not stack
                
